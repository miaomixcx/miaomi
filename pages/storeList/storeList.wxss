/* pages/storeList/storeList.wxss */
page{
  display: flex;
  flex-direction: column;
  height: 100%;
  background: #FAFAFA
}
.header{
  width: 100%;
  height: 130rpx;
  background: #fff;
  padding-top: 26rpx;
  margin-bottom: 20rpx;
}
.headerTitle{
  display: block;
  width: 112rpx;
  height: 40rpx;
  font-size: 28rpx;
  line-height: 40rpx;
  color: rgba(178,178,178,1);
  margin-left: 30rpx;
}
.position{
  width: 22rpx;
  height: 22rpx;
  margin-left: 30rpx;
  margin-top: 30rpx;
  float: left
}
.storeName{
  display: block;
  width: 102rpx;
  height: 48rpx;
  font-size: 34rpx;
  line-height: 48rpx;
  font-weight: 600;
  color: #353535;
  float: left;
  margin-left:8rpx;
  margin-top: 16rpx; 
}
.addressDetail{
  display: block;
  width: 370rpx;
  height: 40rpx;
  font-size: 28rpx;
  line-height: 40rpx;
  font-weight: 400;
  color: #353535;
  float: left;
  margin-left:10rpx;
  margin-top: 22rpx; 
}
.kilometers{
  display: block;
  width: 84rpx;
  height: 34rpx;
  font-size: 24rpx;
  line-height: 34rpx;
  font-weight: 400;
  color: #FF4c4d;
  float: left;
  margin-left:94rpx;
  margin-top: 24rpx; 
}
.main{
  width: 100%;
  height: 100%;
  background: #fff;
}
.mainTitle{
  display: block;
  width: 112rpx;
  height: 40rpx;
  font-size: 28rpx;
  line-height: 40rpx;
  color: rgba(178,178,178,1);
  margin-left: 30rpx;
  margin-top: 26rpx;
}
.storeOne{
  width: 690rpx;
  margin-left: 30rpx;
  height: 132rpx;
  border-bottom: 2rpx solid #F9F9F9;
}
.storeOne .caseA{
  display: flex;
  justify-content: space-between
}
.storeOne .caseB{
  display: flex;
  justify-content: space-between;
  margin-top: 4rpx
}
.storeOneName{
  display: block;
  width: 150rpx;
  height: 48rpx;
  font-size: 34rpx;
  line-height: 48rpx;
  font-weight: 600;
  color: #353535;
  margin-left:30rpx;
  margin-top: 16rpx; 
}
.storeOneKilometers{
  display: block;
  width: 84rpx;
  height: 34rpx;
  font-size: 24rpx;
  line-height: 34rpx;
  font-weight: 400;
  color: #FF4c4d;
  margin-top: 24rpx
}
.storeOneAddressDetail{
  display: block;
  width: 466rpx;
  height: 40rpx;
  font-size: 28rpx;
  line-height: 40rpx;
  font-weight: 400;
  color: #353535;
  margin-left:30rpx;
  margin-top: 4rpx; 
}

.sotreOneViewDetails{
  display: block;
  width: 96rpx;
  height: 34rpx;
  font-size: 24rpx;
  line-height: 34rpx;
  font-weight: 400;
  color: #b2b2b2;
  
  margin-top: 4rpx; 
}
.storeOne:nth-child(2){
  height: 142rpx;
}
.storeOne:nth-child(3){
  height: 142rpx;
}
.storeOne:last-child{
  height: 142rpx;
  border-bottom: none
}
