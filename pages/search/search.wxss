/* pages/search/search.wxss */
page{
  display: flex;
  flex-direction: column;
  height: 100%;
  
}
.main{
  width: 100%;
  height: 100%;
  background: #fff
}
.mainHead{
  width: 690rpx;
  height: 40rpx;
  background: #fff;
  margin-top: 48rpx;
  display: flex;
  justify-content: space-between;
  padding: 0rpx 30rpx;
}
.mainTitle{
  font-size:28rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(53,53,53,1);
  line-height:40rpx;
}
.change{
  font-size:24rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(255,76,77,1);
  line-height:34rpx;
  margin-top: 2rpx
}
.caseList{
  width: 100%;
  height: 100%;
  background: #fff;
  padding-top: 24rpx;
}
.case{
  width:160rpx;
  height:48rpx;
  background:rgba(247,247,247,1);
  border-radius:24rpx;
  font-size:24rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(53,53,53,1);
  line-height:48rpx;
  text-align: center;
  margin-left: 30rpx;
}