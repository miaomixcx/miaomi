/* pages/login/login.wxss */
.header {
    margin: 70rpx 0 90rpx 50rpx;
    border-bottom: 1px solid #ccc;
    text-align: center;
    width: 650rpx;
    height: 300rpx;
}
 
.header image {
    width: 124rpx;
    height: 124rpx;
    margin-bottom: 16rpx;
}

.header text{
  display: block;
  font-size: 36rpx;
  line-height: 50rpx;
  font-weight: 600;
  color: #353535;
}
 
.content {
    margin-left: 50rpx;
    margin-bottom: 90rpx;
}
 
.content text {
    display: block;
    color: #9d9d9d;
    margin-top: 40rpx;
}
 
.bottom {
    border-radius: 80rpx;
    margin: 70rpx 50rpx;
    font-size: 35rpx;
}